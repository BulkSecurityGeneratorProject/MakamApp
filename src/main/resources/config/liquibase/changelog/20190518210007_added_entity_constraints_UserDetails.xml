<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity UserDetails.
    -->
    <changeSet id="20190518210007-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_details_extras_id"
                                 baseTableName="user_details"
                                 constraintName="fk_user_details_user_details_extras_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_details_extras"/>

        <addForeignKeyConstraint baseColumnNames="user_details_id"
                                 baseTableName="user_details_achievement_dictionary"
                                 constraintName="fk_user_details_achievement_dictionary_user_details_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_details"/>

        <addForeignKeyConstraint baseColumnNames="achievement_dictionary_id"
                                 baseTableName="user_details_achievement_dictionary"
                                 constraintName="user_details_achievement_dicti_achievement_dictionary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="achievement_dictionary"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="user_details"
                                 constraintName="fk_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
        
    </changeSet>
</databaseChangeLog>
